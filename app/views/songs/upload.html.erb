<h1>Upload new song</h1>

<%= form_for @song do |f| %>
	<%= f.label :name%>
	<%= f.text_field :name %>
	<br>
	<br>

	<%= f.label "Artist" %>
	<%= f.text_field :artist_name %>
	<br>
	<br>
	<%= f.label "Genre" %>
	<%= f.text_field :genre_name%>
	<br>
	<br>
	<%= f.label "Audio File" %>
	<%= f.file_field :song_file %>

<%= f.submit %>
<% end %>